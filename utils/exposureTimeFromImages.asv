function varargout = exposureTimeFromImages(trial,imdir)

fprintf(1,'\t**Making exposure timeline from files: \n\t%s\n',imdir);
imagefiles = dir(fullfile(imdir,[trial.params.protocol '_Image_*']));
frametime = zeros(1,length(imagefiles));
for i_ind = 1:length(imagefiles)
    frametime(i_ind) = imagefiles(i_ind).datenum;
    [I(:,:,1,frame)] = imread(fullfile(imdir,imagefiles(frame).name));
end
varargout = {trial.exposure_time,trial.exposure};